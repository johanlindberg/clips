(unwatch all)
(clear)
(defglobal ?*sorstate* = (set-sequence-operator-recognition TRUE))
(dribble-on "dfnxerr.out")
(batch "dfnxerr.bat")
(dribble-off)
(set-sequence-operator-recognition ?*sorstate*))
(clear)
(open "dfnxerr.rsl" dfnxerr "w")
(load "compline.clp")
(printout dfnxerr "dfnxerr.bat differences are as follows:" crlf)
(compare-files dfnxerr.exp dfnxerr.out dfnxerr)
(close dfnxerr)
