(unwatch all)
(clear)
(conserve-mem off)
(dribble-on "coolcmd.out")
(batch "coolcmd.bat")
(dribble-off)
(clear)
(open "coolcmd.rsl" coolcmd "w")
(load "compline.clp")
(printout coolcmd "coolcmd.bat differences are as follows:" crlf)
(compare-files coolcmd.exp coolcmd.out coolcmd)
(printout coolcmd "coolcmd.ins differences are as follows:" crlf)
(compare-files coolcmd.tmp coolcmd.dat coolcmd)
(close coolcmd)
